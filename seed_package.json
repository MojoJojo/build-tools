{
    "name": "CHANGEME",
    "mainFile": "CHANGEME",
    "scripts": {
        "dev": "gulp --gulpfile node_modules/build-tools/Gulpfile.js --cwd ./ watch --watch --host localhost",
        "setup": "npm install && npm run update && bower install --allow-root && rm -rf build && npm run sprites && npm run dependencies",
        "dependencies": "gulp --gulpfile node_modules/build-tools/Gulpfile.js --cwd ./ dependencies --env production",
        "update": "sh node_modules/build-tools/npm-install.sh",
        "full": "npm run setup && npm run build && npm run test",
        "sprites": "gulp --gulpfile node_modules/build-tools/Gulpfile.js --cwd ./ sprites",
        "build": "gulp --gulpfile node_modules/build-tools/Gulpfile.js --cwd ./ package --env production",
        "test:build": "gulp --gulpfile node_modules/build-tools/Gulpfile.js --cwd ./ compile-tests",
        "test:local:run": "karma start --browsers=Chrome --host=localhost",
        "test:local": "npm run test:build && npm run test:local:run",
        "test": "npm run test:build && karma start --single-run --reporters=junit,coverage,html,verbose"
    },
    "version": "0.0.1",
    "devDependencies": {
        "build-tools": "egis/build-tools",
        "gulp": "^3.9.0"
    }
}
